#ifndef libunwind_obj_h
#define libunwind_obj_h

#define UNW_TARGET		@arch@
#define UNW_TARGET_@ARCH@	1

#define UNW_HAVE_SYMBOL_PREFIX	@HAVE_SYMBOL_PREFIX@
#define UNW_SYMPREFIX		UNW_PASTE(@symbol_prefix@,_U)

#define UNW_PASTE2(x,y)	x##y
#define UNW_PASTE(x,y)	UNW_PASTE2(x,y)
#define UNW_OBJ(fn)	UNW_PASTE(UNW_PREFIX, fn)
#define UNW_ARCH_OBJ(fn) UNW_PASTE(UNW_PASTE(UNW_PASTE(UNW_SYMPREFIX,UNW_TARGET),_), fn)

#ifdef UNW_LOCAL_ONLY
# define UNW_PREFIX	UNW_PASTE(UNW_PASTE(UNW_PASTE(UNW_SYMPREFIX,L),UNW_TARGET),_)
#else /* !UNW_LOCAL_ONLY */
# define UNW_PREFIX	UNW_PASTE(UNW_PASTE(UNW_SYMPREFIX,UNW_TARGET),_)
#endif /* !UNW_LOCAL_ONLY */

#endif /* libunwind_obj_h */
